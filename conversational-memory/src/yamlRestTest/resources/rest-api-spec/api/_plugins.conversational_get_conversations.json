{
    "_plugins.conversational_get_conversations": {
        "stability": "stable",
        "url": {
            "paths": [
                {
                    "path": "/_plugins/ml/conversational/memory",
                    "methods": ["GET"]
                }
            ]
        },
        "params": {
            "maxResults": {
                "type": "number",
                "description": "[optional] number of results to return (defaults to 10)"
            },
            "nextToken": {
                "type": "number",
                "description": "[optional] token pointing to the next page of results"
            }
        }
    }
}