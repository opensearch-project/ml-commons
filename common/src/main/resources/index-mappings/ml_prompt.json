{
  "_meta": {
    "schema_version": 1
  },
  "properties": {
    "name": {
      "type": "text",
      "fields": {
        "keyword": {
          "type": "keyword",
          "ignore_above": 256
        }
      }
    },
    "description": {
      "type": "text"
    },
    "version": {
      "type": "keyword"
    },
    "prompt": {
      "type": "flat_object"
    },
    "tags": {
      "type": "keyword"
    },
    "tenant_id": {
      "type": "keyword"
    },
    "create_time": {
      "type": "date",
      "format": "strict_date_time||epoch_millis"
    },
    "last_update_time": {
      "type": "date",
      "format": "strict_date_time||epoch_millis"
    }
  }
}